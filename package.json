{
  "name": "outerbase-due",
  "description": "A TypeScript-first system for working with Outerbase Web/Node.js project",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "build": "rimraf dist && tsc -p tsconfig.cjs.json && tsc -p tsconfig.esm.json && bash ./fixup.sh",
    "eslint:check": "eslint ./ --ext .js,.jsx,.ts,.tsx",
    "eslint:fix": "eslint ./ --fix --ext .js,.jsx,.ts,.tsx",
    "outerbase:command:generator:compile": "rollup -i src/outerbase/system/command/generator.ts -o outerbase/dist/command.generator.js -f cjs -c src/outerbase/system/rollup.config.mjs",
    "outerbase:command:generator:exec": "npm run outerbase:command:generator:compile && npm run outerbase:command:generator:run",
    "outerbase:command:generator:run": "node outerbase/dist/command.generator.js",
    "outerbase:plugin:generator:compile": "rollup -i src/outerbase/system/plugin/generator.ts -o outerbase/dist/plugin.generator.js -f cjs -c src/outerbase/system/rollup.config.mjs",
    "outerbase:plugin:generator:exec": "npm run outerbase:plugin:generator:compile && npm run outerbase:plugin:generator:run",
    "outerbase:plugin:generator:run": "node outerbase/dist/plugin.generator.js",
    "prepare": "husky install",
    "prettier:check": "prettier --check .",
    "prettier:write": "prettier --write .",
    "type:check": "tsc -p tsconfig.json"
  },
  "dependencies": {
    "@rollup/plugin-node-resolve": "^15.2.1",
    "@rollup/plugin-replace": "^5.0.2",
    "@rollup/plugin-terser": "^0.4.3",
    "@rollup/plugin-virtual": "^3.0.1",
    "@types/clean-css": "^4.2.7",
    "@types/node": "20.6.0",
    "clean-css": "^5.3.2",
    "construct-style-sheets-polyfill": "^3.1.0",
    "dotenv": "^16.3.1",
    "postcss": "^8.4.29",
    "prettier": "^3.0.3",
    "rimraf": "^5.0.4",
    "rollup": "^3.29.2",
    "rollup-plugin-typescript2": "^0.35.0",
    "tailwindcss": "^3.3.3",
    "terser": "^5.20.0",
    "typescript": "5.2.2"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^6.6.0",
    "@typescript-eslint/parser": "^6.6.0",
    "eslint": "^8.2.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-airbnb-typescript": "^17.1.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-import-resolver-alias": "^1.1.2",
    "eslint-plugin-eslint-comments": "^3.2.0",
    "eslint-plugin-ignore-generated": "^3.1.0",
    "eslint-plugin-import": "^2.25.2",
    "husky": "^8.0.3",
    "lint-staged": "^14.0.1",
    "prettier-plugin-tailwindcss": "^0.5.4"
  }
}
